int simOUnao()
{
    char resposta[71];
    int i, simOUnao, aux;
    do
    {
        aux = 0;
        gets(resposta);

        for (i = 0; resposta[i] != '\0'; i++)
        {
            resposta[i] = tolower(resposta[i]);
        }

        if ((strcmp(resposta, "s") == 0 || strcmp(resposta, "sim") == 0))
        {
            simOUnao = 1;
        } // sim
        else if ((strcmp(resposta, "n") == 0 || strcmp(resposta, "nao") == 0))
        {
            simOUnao = 0;
        } // nao
        else
        {
            printf("\nErro! Responda Apenas com 'SIM' ou 'NAO'.");
            aux = 1;
        } // erro

    } while (aux == 1 && printf("\nDigite novamente: "));
    printf("\n");
    return simOUnao;
}

