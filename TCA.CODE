                }

                if (_ConjuntoDeClas[OpçaoDeCla - 1].JutsusTradicionais[i]->QuantidadeDeElementos > 0 && _ConjuntoDeClas[OpçaoDeCla - 1].JutsusTradicionais[i]->Elementos != NULL)
