#else
        sleep(1);
#endif

        printf(".");
    }
    printf("\n");
}

void limpar_tela()
{
#ifdef _WIN32
    system("cls"); // Windows
#else
    system("clear"); // Linux / macOS
#endif
}

void congelar_tela()
{
#ifdef _WIN32
    printf("\n");
