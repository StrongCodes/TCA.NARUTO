                    else
                    {
                        free(_ConjuntoDeMissoes[idx].Titulo);
                        _ConjuntoDeMissoes[idx].Titulo = malloc(strlen(strAux) + 1);
                        strcpy(_ConjuntoDeMissoes[idx].Titulo, strAux);
                        printf("\nTítulo da missão alterado com sucesso!\n");
                    }
                    break;

                case 2:
                    printf("\n--- Alterar Líder da Missão ---\nLíder atual: %s\nSelecione o novo líder:\n", _ConjuntoDeMissoes[idx].LiderResponsavel->Nome);
                    if (_NumeroDoShinobi == 0)
                    {
                        printf("Nenhum shinobi cadastrado!\n");
                    }
                    else
                    {
                        int liderIdx = EscolheOpçao(1) - 1;
                        if (liderIdx >= 0)
                        {
                            free(_ConjuntoDeMissoes[idx].LiderResponsavel->Nome);
                            _ConjuntoDeMissoes[idx].LiderResponsavel->Nome = malloc(strlen(_ConjuntoDeShinobis[liderIdx].Nome) + 1);
                            strcpy(_ConjuntoDeMissoes[idx].LiderResponsavel->Nome, _ConjuntoDeShinobis[liderIdx].Nome);
                            printf("\nLíder da missão alterado com sucesso!\n");
                        }
                    }
                    break;

                case 3:
                    printf("\n--- Alterar Nível de Dificuldade ---\nNível atual: %s\nNovo nível (D/C/B/A/S): ", _ConjuntoDeMissoes[idx].NivelDificuldade);
                    VerificaNivelDeDificuldade(strAux);
                    free(_ConjuntoDeMissoes[idx].NivelDificuldade);
                    _ConjuntoDeMissoes[idx].NivelDificuldade = malloc(strlen(strAux) + 1);
                    strcpy(_ConjuntoDeMissoes[idx].NivelDificuldade, strAux);
                    printf("\nNível de dificuldade alterado com sucesso!\n");
                    break;

                case 4:
                    printf("\n--- Alterar Status da Missão ---\nStatus atual: %s\nNovo status:\n{ 1 } Pendente\n{ 2 } Em Andamento\n{ 3 } Concluída\n{ 4 } Falha\n{ 5 } Cancelada\nOpção: ", _ConjuntoDeMissoes[idx].Status);
                    SelecionaStatus(VereficaQuantia(1, 5), strAux);
                    free(_ConjuntoDeMissoes[idx].Status);
                    _ConjuntoDeMissoes[idx].Status = malloc(strlen(strAux) + 1);
                    strcpy(_ConjuntoDeMissoes[idx].Status, strAux);
                    printf("\nStatus da missão alterado com sucesso!\n");
                    break;

                case 5:
                    printf("\n--- Gerenciar Ninjas da Missão ---\n");
                    if (_ConjuntoDeMissoes[idx].QuantidadeDeNinjas > 0)
                    {
                        printf("Ninjas atuais na missão:\n");
                        for (int i = 0; i < _ConjuntoDeMissoes[idx].QuantidadeDeNinjas; i++)
                        {
                            printf("{ %d } - %s\n", i + 1, _ConjuntoDeMissoes[idx].NinjasDaMissao[i]->Nome);
                        }
                        printf("\n{ 1 } - Adicionar Ninja\n{ 2 } - Remover Ninja\n{ 3 } - Alterar Ninja\nOpção: ");
                        int opNinja = VereficaQuantia(1, 3);
                        if (opNinja == 1)
                        {
                            printf("Selecione ninja para adicionar:\n");
                            int ninjaIdx = EscolheOpçao(1) - 1;
                            if (ninjaIdx >= 0)
                            {
                                _ConjuntoDeMissoes[idx].QuantidadeDeNinjas++;
                                _ConjuntoDeMissoes[idx].NinjasDaMissao = realloc(_ConjuntoDeMissoes[idx].NinjasDaMissao, _ConjuntoDeMissoes[idx].QuantidadeDeNinjas * sizeof(tNinja *));
                                _ConjuntoDeMissoes[idx].NinjasDaMissao[_ConjuntoDeMissoes[idx].QuantidadeDeNinjas - 1] = malloc(sizeof(tNinja));
                                _ConjuntoDeMissoes[idx].NinjasDaMissao[_ConjuntoDeMissoes[idx].QuantidadeDeNinjas - 1]->Nome = malloc(strlen(_ConjuntoDeShinobis[ninjaIdx].Nome) + 1);
                                strcpy(_ConjuntoDeMissoes[idx].NinjasDaMissao[_ConjuntoDeMissoes[idx].QuantidadeDeNinjas - 1]->Nome, _ConjuntoDeShinobis[ninjaIdx].Nome);
                                printf("Ninja adicionado com sucesso!\n");
                            }
                        }
                        else if (opNinja == 2)
                        {
                            printf("Qual ninja remover? (1-%d): ", _ConjuntoDeMissoes[idx].QuantidadeDeNinjas);
                            int remIdx = VereficaQuantia(1, _ConjuntoDeMissoes[idx].QuantidadeDeNinjas) - 1;
                            free(_ConjuntoDeMissoes[idx].NinjasDaMissao[remIdx]->Nome);
                            free(_ConjuntoDeMissoes[idx].NinjasDaMissao[remIdx]);
                            for (int i = remIdx; i < _ConjuntoDeMissoes[idx].QuantidadeDeNinjas - 1; i++)
                            {
                                _ConjuntoDeMissoes[idx].NinjasDaMissao[i] = _ConjuntoDeMissoes[idx].NinjasDaMissao[i + 1];
                            }
                            _ConjuntoDeMissoes[idx].QuantidadeDeNinjas--;
                            printf("Ninja removido com sucesso!\n");
                        }
                        else if (opNinja == 3)
                        {
                            printf("Qual ninja alterar? (1-%d): ", _ConjuntoDeMissoes[idx].QuantidadeDeNinjas);
                            int altIdx = VereficaQuantia(1, _ConjuntoDeMissoes[idx].QuantidadeDeNinjas) - 1;
                            printf("Novo nome para o ninja: ");
                            VerificadorDeTexto(strAux);
                            free(_ConjuntoDeMissoes[idx].NinjasDaMissao[altIdx]->Nome);
                            _ConjuntoDeMissoes[idx].NinjasDaMissao[altIdx]->Nome = malloc(strlen(strAux) + 1);
                            strcpy(_ConjuntoDeMissoes[idx].NinjasDaMissao[altIdx]->Nome, strAux);
                            printf("Ninja alterado com sucesso!\n");
                        }
                    }
                    else
                    {
                        printf("Esta missão não possui ninjas.\n{ 1 } - Adicionar Ninja\n{ 2 } - Voltar\nOpção: ");
                        if (VereficaQuantia(1, 2) == 1)
                        {
                            printf("Selecione ninja para adicionar:\n");
                            int ninjaIdx = EscolheOpçao(1) - 1;
                            if (ninjaIdx >= 0)
                            {
                                _ConjuntoDeMissoes[idx].QuantidadeDeNinjas = 1;
                                _ConjuntoDeMissoes[idx].NinjasDaMissao = malloc(sizeof(tNinja *));
                                _ConjuntoDeMissoes[idx].NinjasDaMissao[0] = malloc(sizeof(tNinja));
                                _ConjuntoDeMissoes[idx].NinjasDaMissao[0]->Nome = malloc(strlen(_ConjuntoDeShinobis[ninjaIdx].Nome) + 1);
                                strcpy(_ConjuntoDeMissoes[idx].NinjasDaMissao[0]->Nome, _ConjuntoDeShinobis[ninjaIdx].Nome);
                                printf("Ninja adicionado com sucesso!\n");
                            }
                        }
                    }
                    break;
                }
                break;
            }

            printf("\n");
            congelar_tela();

        } while (1);

        printf("\nDeseja alterar outro %s? (s/n): ", tipoNome);
        if (simOUnao() == 0)
        {
            break;
        }
    }
}

void Remover(int TipoDeContexto)
{
    int opcao, indice;
    char *tipoNome;

    while (1)
    {
        limpar_tela();
        printf("\n=== REMOVER ===\n");

        switch (TipoDeContexto)
        {
        case 1:
            if (_NumeroDoShinobi == 0)
            {
                printf("Nenhum shinobi cadastrado!\n");
                congelar_tela();
                return;
            }
            printf("Shinobis:\n");
            for (int i = 0; i < _NumeroDoShinobi; i++)
            {
                printf("{ %d } - %s\n", i + 1, _ConjuntoDeShinobis[i].Nome);
            }
            tipoNome = "shinobi";
            break;

        case 2:
            if (_NumeroDoCla == 0)
            {
                printf("Nenhum clã cadastrado!\n");
                congelar_tela();
                return;
            }
            printf("Clãs:\n");
            for (int i = 0; i < _NumeroDoCla; i++)
            {
                printf("{ %d } - %s\n", i + 1, _ConjuntoDeClas[i].NomeDoCla);
            }
            tipoNome = "clã";
            break;

        case 3:
            if (_NumeroDoJutsu == 0)
            {
                printf("Nenhum jutsu cadastrado!\n");
                congelar_tela();
                return;
            }
            printf("Jutsus:\n");
            for (int i = 0; i < _NumeroDoJutsu; i++)
            {
                printf("{ %d } - %s\n", i + 1, _ConjuntoDeJutsus[i].Nome);
            }
            tipoNome = "jutsu";
            break;

        case 4:
            if (_NumeroDaMissao == 0)
            {
                printf("Nenhuma missão cadastrada!\n");
                congelar_tela();
                return;
            }
            printf("Missões:\n");
            for (int i = 0; i < _NumeroDaMissao; i++)
            {
                printf("{ %d } - %s\n", i + 1, _ConjuntoDeMissoes[i].Titulo);
            }
            tipoNome = "missão";
            break;
        }

        printf("\nQual %s remover? (0=Voltar): ", tipoNome);
        indice = VereficaQuantia(0, (TipoDeContexto == 1) ? _NumeroDoShinobi : (TipoDeContexto == 2) ? _NumeroDoCla
                                                                           : (TipoDeContexto == 3)   ? _NumeroDoJutsu
                                                                                                     : _NumeroDaMissao);
        if (indice == 0)
            return;

        int idx = indice - 1;

        limpar_tela();
        printf("\n=== REMOVER %s ===\n", tipoNome);
        RelatorioDaOpçao(TipoDeContexto, indice, 1);

        printf("\nComo remover?\n{ 1 } - Remover %s completo\n{ 2 } - Remover partes específicas\n{ 3 } - Cancelar\nOpção: ", tipoNome);
        opcao = VereficaQuantia(1, 3);

        if (opcao == 3)
            continue;

        if (opcao == 1)
        {
            printf("\nTem certeza? (s/n): ");
            if (simOUnao() == 1)
            {
                switch (TipoDeContexto)
                {
                case 1:
                    // REMOVER SHINOBI DE MISSÕES
                    for (int i = 0; i < _NumeroDaMissao; i++)
                    {
                        for (int j = 0; j < _ConjuntoDeMissoes[i].QuantidadeDeNinjas; j++)
                        {
                            if (strcmp(_ConjuntoDeMissoes[i].NinjasDaMissao[j]->Nome, _ConjuntoDeShinobis[idx].Nome) == 0)
                            {
                                free(_ConjuntoDeMissoes[i].NinjasDaMissao[j]->Nome);
                                free(_ConjuntoDeMissoes[i].NinjasDaMissao[j]);
                                for (int k = j; k < _ConjuntoDeMissoes[i].QuantidadeDeNinjas - 1; k++)
                                {
                                    _ConjuntoDeMissoes[i].NinjasDaMissao[k] = _ConjuntoDeMissoes[i].NinjasDaMissao[k + 1];
                                }
                                _ConjuntoDeMissoes[i].QuantidadeDeNinjas--;
                                break;
                            }
                        }
                    }
                    // LIBERAR MEMÓRIA DO SHINOBI
                    free(_ConjuntoDeShinobis[idx].Nome);
                    free(_ConjuntoDeShinobis[idx].Patente);
                    free(_ConjuntoDeShinobis[idx].ElementoPrincipal);
                    if (_ConjuntoDeShinobis[idx].CaracteristicasDoCla)
                    {
                        free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->NomeDoCla);
                        if (_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios)
                        {
                            for (int i = 0; i < _ConjuntoDeShinobis[idx].CaracteristicasDoCla->QuantidadeDeJutsusHereditarios; i++)
                            {
                                free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios[i]->Nome);
                                free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios[i]->TipoJutsu);
                                if (_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios[i]->Elementos)
                                {
                                    for (int j = 0; j < _ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios[i]->QuantidadeDeElementos; j++)
                                    {
                                        free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios[i]->Elementos[j]);
                                    }
                                    free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios[i]->Elementos);
                                }
                                free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios[i]);
                            }
                            free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusHereditarios);
                        }
                        if (_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais)
                        {
                            for (int i = 0; i < _ConjuntoDeShinobis[idx].CaracteristicasDoCla->QuantidadeDeJutsusTradicionais; i++)
                            {
                                free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais[i]->Nome);
                                free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais[i]->TipoJutsu);
                                if (_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais[i]->Elementos)
                                {
                                    for (int j = 0; j < _ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais[i]->QuantidadeDeElementos; j++)
                                    {
                                        free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais[i]->Elementos[j]);
                                    }
                                    free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais[i]->Elementos);
                                }
                                free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais[i]);
                            }
                            free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla->JutsusTradicionais);
                        }
                        free(_ConjuntoDeShinobis[idx].CaracteristicasDoCla);
                    }
                    if (_ConjuntoDeShinobis[idx].CaracteristicasDoJutsu)
                    {
                        for (int i = 0; i < _ConjuntoDeShinobis[idx].QuantidadeDeJutsus; i++)
                        {
                            free(_ConjuntoDeShinobis[idx].CaracteristicasDoJutsu[i]->Nome);
                            free(_ConjuntoDeShinobis[idx].CaracteristicasDoJutsu[i]->TipoJutsu);
                            if (_ConjuntoDeShinobis[idx].CaracteristicasDoJutsu[i]->Elementos)
                            {
                                for (int j = 0; j < _ConjuntoDeShinobis[idx].CaracteristicasDoJutsu[i]->QuantidadeDeElementos; j++)
                                {
                                    free(_ConjuntoDeShinobis[idx].CaracteristicasDoJutsu[i]->Elementos[j]);
                                }
                                free(_ConjuntoDeShinobis[idx].CaracteristicasDoJutsu[i]->Elementos);
                            }
                            free(_ConjuntoDeShinobis[idx].CaracteristicasDoJutsu[i]);
                        }
                        free(_ConjuntoDeShinobis[idx].CaracteristicasDoJutsu);
                    }
                    for (int i = idx; i < _NumeroDoShinobi - 1; i++)
                    {
                        _ConjuntoDeShinobis[i] = _ConjuntoDeShinobis[i + 1];
                    }
                    _NumeroDoShinobi--;
                    break;

                case 2:
                    // REMOVER CLÃ DE SHINOBIS
                    for (int i = 0; i < _NumeroDoShinobi; i++)
                    {
                        if (_ConjuntoDeShinobis[i].CaracteristicasDoCla && strcmp(_ConjuntoDeShinobis[i].CaracteristicasDoCla->NomeDoCla, _ConjuntoDeClas[idx].NomeDoCla) == 0)
                        {
                            free(_ConjuntoDeShinobis[i].CaracteristicasDoCla->NomeDoCla);
                            _ConjuntoDeShinobis[i].CaracteristicasDoCla->NomeDoCla = malloc(strlen("Sem Clã") + 1);
                            strcpy(_ConjuntoDeShinobis[i].CaracteristicasDoCla->NomeDoCla, "Sem Clã");
                        }
                    }
                    // LIBERAR MEMÓRIA DO CLÃ
                    free(_ConjuntoDeClas[idx].NomeDoCla);
                    if (_ConjuntoDeClas[idx].JutsusHereditarios)
                    {
                        for (int i = 0; i < _ConjuntoDeClas[idx].QuantidadeDeJutsusHereditarios; i++)
                        {
                            free(_ConjuntoDeClas[idx].JutsusHereditarios[i]->Nome);
                            free(_ConjuntoDeClas[idx].JutsusHereditarios[i]->TipoJutsu);
                            if (_ConjuntoDeClas[idx].JutsusHereditarios[i]->Elementos)
                            {
                                for (int j = 0; j < _ConjuntoDeClas[idx].JutsusHereditarios[i]->QuantidadeDeElementos; j++)
                                {
                                    free(_ConjuntoDeClas[idx].JutsusHereditarios[i]->Elementos[j]);
                                }
                                free(_ConjuntoDeClas[idx].JutsusHereditarios[i]->Elementos);
                            }
                            free(_ConjuntoDeClas[idx].JutsusHereditarios[i]);
                        }
                        free(_ConjuntoDeClas[idx].JutsusHereditarios);
                    }
                    if (_ConjuntoDeClas[idx].JutsusTradicionais)
                    {
                        for (int i = 0; i < _ConjuntoDeClas[idx].QuantidadeDeJutsusTradicionais; i++)
                        {
                            free(_ConjuntoDeClas[idx].JutsusTradicionais[i]->Nome);
                            free(_ConjuntoDeClas[idx].JutsusTradicionais[i]->TipoJutsu);
                            if (_ConjuntoDeClas[idx].JutsusTradicionais[i]->Elementos)
                            {
                                for (int j = 0; j < _ConjuntoDeClas[idx].JutsusTradicionais[i]->QuantidadeDeElementos; j++)
                                {
                                    free(_ConjuntoDeClas[idx].JutsusTradicionais[i]->Elementos[j]);
                                }
                                free(_ConjuntoDeClas[idx].JutsusTradicionais[i]->Elementos);
                            }
                            free(_ConjuntoDeClas[idx].JutsusTradicionais[i]);
                        }
                        free(_ConjuntoDeClas[idx].JutsusTradicionais);
                    }
                    for (int i = idx; i < _NumeroDoCla - 1; i++)
                    {
                        _ConjuntoDeClas[i] = _ConjuntoDeClas[i + 1];
                    }
                    _NumeroDoCla--;
                    break;

                case 3:
                    // REMOVER JUTSU DE SHINOBIS E CLÃS
                    for (int i = 0; i < _NumeroDoShinobi; i++)
                    {
                        for (int j = 0; j < _ConjuntoDeShinobis[i].QuantidadeDeJutsus; j++)
                        {
                            if (strcmp(_ConjuntoDeShinobis[i].CaracteristicasDoJutsu[j]->Nome, _ConjuntoDeJutsus[idx].Nome) == 0)
                            {
                                free(_ConjuntoDeShinobis[i].CaracteristicasDoJutsu[j]->Nome);
                                free(_ConjuntoDeShinobis[i].CaracteristicasDoJutsu[j]->TipoJutsu);
                                if (_ConjuntoDeShinobis[i].CaracteristicasDoJutsu[j]->Elementos)
                                {
                                    for (int k = 0; k < _ConjuntoDeShinobis[i].CaracteristicasDoJutsu[j]->QuantidadeDeElementos; k++)
                                    {
                                        free(_ConjuntoDeShinobis[i].CaracteristicasDoJutsu[j]->Elementos[k]);
                                    }
                                    free(_ConjuntoDeShinobis[i].CaracteristicasDoJutsu[j]->Elementos);
                                }
                                free(_ConjuntoDeShinobis[i].CaracteristicasDoJutsu[j]);
                                for (int k = j; k < _ConjuntoDeShinobis[i].QuantidadeDeJutsus - 1; k++)
                                {
                                    _ConjuntoDeShinobis[i].CaracteristicasDoJutsu[k] = _ConjuntoDeShinobis[i].CaracteristicasDoJutsu[k + 1];
                                }
                                _ConjuntoDeShinobis[i].QuantidadeDeJutsus--;
                                break;
                            }
                        }
                    }
                    // LIBERAR MEMÓRIA DO JUTSU
                    free(_ConjuntoDeJutsus[idx].Nome);
                    free(_ConjuntoDeJutsus[idx].TipoJutsu);
                    if (_ConjuntoDeJutsus[idx].Elementos)
                    {
                        for (int i = 0; i < _ConjuntoDeJutsus[idx].QuantidadeDeElementos; i++)
                        {
                            free(_ConjuntoDeJutsus[idx].Elementos[i]);
                        }
                        free(_ConjuntoDeJutsus[idx].Elementos);
                    }
                    for (int i = idx; i < _NumeroDoJutsu - 1; i++)
                    {
                        _ConjuntoDeJutsus[i] = _ConjuntoDeJutsus[i + 1];
                    }
                    _NumeroDoJutsu--;
                    break;

                case 4:
                    // LIBERAR MEMÓRIA DA MISSÃO
                    free(_ConjuntoDeMissoes[idx].Titulo);
                    free(_ConjuntoDeMissoes[idx].NivelDificuldade);
                    free(_ConjuntoDeMissoes[idx].Status);
                    if (_ConjuntoDeMissoes[idx].LiderResponsavel)
                    {
