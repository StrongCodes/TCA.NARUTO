                for (int j = 0; j < QuantidadeDeJutsu; j++)
                {
                    AuxTok = strtok(NULL, ",");
                    _ConjuntoDeJutsus[_NumeroDoJutsu].Nome = (char *)malloc((strlen(AuxTok) + 1) * sizeof(char));
                    strcpy(_ConjuntoDeJutsus[_NumeroDoJutsu].Nome, AuxTok);

                    AuxTok = strtok(NULL, ",");
