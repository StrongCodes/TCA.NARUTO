                            free(_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoCla);
                        }
                        if (_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu)
                        {
                            for (int i = 0; i < _ConjuntoDeMissoes[idx].LiderResponsavel->QuantidadeDeJutsus; i++)
                            {
                                free(_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu[i]->Nome);
                                free(_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu[i]->TipoJutsu);
                                if (_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu[i]->Elementos)
                                {
                                    for (int j = 0; j < _ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu[i]->QuantidadeDeElementos; j++)
                                    {
                                        free(_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu[i]->Elementos[j]);
                                    }
                                    free(_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu[i]->Elementos);
                                }
                                free(_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu[i]);
                            }
                            free(_ConjuntoDeMissoes[idx].LiderResponsavel->CaracteristicasDoJutsu);
                        }
                        free(_ConjuntoDeMissoes[idx].LiderResponsavel);
                    }
                    if (_ConjuntoDeMissoes[idx].NinjasDaMissao)
                    {
                        for (int i = 0; i < _ConjuntoDeMissoes[idx].QuantidadeDeNinjas; i++)
                        {
                            free(_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->Nome);
                            free(_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->Patente);
                            free(_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->ElementoPrincipal);
                            if (_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->CaracteristicasDoCla)
                            {
                                free(_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->CaracteristicasDoCla->NomeDoCla);
                                free(_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->CaracteristicasDoCla);
                            }
                            if (_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->CaracteristicasDoJutsu)
                            {
                                for (int j = 0; j < _ConjuntoDeMissoes[idx].NinjasDaMissao[i]->QuantidadeDeJutsus; j++)
                                {
                                    free(_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->CaracteristicasDoJutsu[j]->Nome);
                                    free(_ConjuntoDeMissoes[idx].NinjasDaMissao[i]->CaracteristicasDoJutsu[j]->TipoJutsu);
